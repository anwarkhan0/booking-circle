<!-- Primary Page Layout  -->

<div class="section big-55-height over-hide z-bigger customClass">

    <div class="parallax parallax-top" style="background-image: url('/img/rooms.jpg')"></div>
    <div class="dark-over-pages"></div>

    <div class="hero-center-section pages">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-12 parallax-fade-top">
                    <div class="hero-text"><%= hotel.name %></div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="section padding-top-bottom over-hide">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-md-8 align-self-center">
                <div class="subtitle with-line text-center mb-4">gallery</div>
                <h3 class="text-center padding-bottom-small"><%= hotel.name %> </h3>
            </div>
            <div class="section clearfix"></div>
            <% for(let i= 0; i< hotel.gallery.length; i++){ %> 
            <div class="col-md-4 mt-4">
                <a href="/uploads/<%= hotel.gallery[i] %>" data-fancybox=" gallery">
                    <div class="img-wrap gallery-small">
                        <img src="/uploads/<%= hotel.gallery[i] %>" alt="">
                    </div>
                </a>
            </div>
            <% } %> 
            
        </div>
    </div>
</div>