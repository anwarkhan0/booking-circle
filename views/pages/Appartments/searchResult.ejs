
    <div class="section padding-top-bottom over-hide background-grey">
        <div class="container">
            <div class="row justify-content-center">
    
                <% for(let i= 0; i< appartments.length; i++){ %> 
                <div class="col-md-4 mt-4 mt-md-0" data-scroll-reveal="enter bottom move 50px over 0.7s after 0.2s">
                    <div class="room-box background-white">
                        <img src="../img/room1.jpg" alt="">
                        <div class="room-box-in">
                            <h5 class=""><%= appartments[i].name %></h5>
                            <p class="mt-3"><strong>Address: </strong> <%= appartments[i].address %> </p>
                            <a class="mt-1 btn btn-primary" href="/Appartments/apartmentBooking/<%= appartments[i].id %>">Select Apartment</a>
                            <!-- <a class="mt-1 btn btn-primary" href="/Appartments/appartmentGallery">Gallery</a> -->
                        </div>
                    </div>
                </div>
                <% } %> 
    
            </div>
        </div>
    </div>