<!-- Primary Page Layout
	================================================== -->
<div class="section big-55-height over-hide z-bigger customClass">

	<div class="parallax parallax-top" style="background-image: url('../img/car.jpg')"></div>
	<div class="dark-over-pages"></div>

	<div class="hero-center-section pages">
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-12 parallax-fade-top">
					<div class="hero-text">Rental Vehicles</div>
				</div>
			</div>
		</div>
	</div>
</div>

<div class="container-fluid sliderContainerClass p-5" id="fixed">
	<div class="row">
		<p id="error" class="text-danger font-weight-bold" style="display: none;">Please provide the values.</p>
		<div class="col-12 mt-3">
			<div class="booking-hero-wrap customWrapper">
				<div class="row justify-content-center customJustifier">
					<div class="col-md-3 col-sm-12" id="dateId">
                        <div class="input-daterange input-group" id="flight-datepicker">
                            <div class="row">
                                <div class="col-6">
                                    <div class="form-item inputClass">
                                        <span class="fontawesome-calendar"></span>
                                        <input class="input-sm" type="text" autocomplete="off" id="start-date-1"
                                            name="start" placeholder="chech-in date" data-date-format="DD-MM-d" />
                                        <span class="date-text date-depart"></span>
                                    </div>
                                </div>
                                <div class="col-6">
                                    <div class="form-item inputClass">
                                        <span class="fontawesome-calendar"></span>
                                        <input class="input-sm" type="text" autocomplete="off" id="end-date-1"
                                            name="end" placeholder="check-out date" data-date-format="DD-MM-d" />
                                        <span class="date-text date-return"></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
					<div class="col-md-2 col-sm-1 customCol">
						<select class="wide nice-select" id="pickup">
							<option data-display="Pickup Location" value=""></option>
							<% for(let i= 0; i< areas.length; i++){ %> 
							<option value="<%= areas[i].name %>"><%= areas[i].name %></option>
							<% } %> 
						</select>
					</div>

					<div class="col-md-2 col-sm-1 customCol">
						<select class="wide nice-select" id="dropOff">
							<option data-display="drop Off Location" value=""></option>
							<% for(let i= 0; i< areas.length; i++){ %> 
							<option value="<%= areas[i].name %>"><%= areas[i].name %></option>
							<% } %> 
						</select>
					</div>


					<div class="col-md-2 col-sm-6 customCol">
						<select name="adults" class="wide" id="adultsForVehicle">
							<option data-display="adults" value="false">adults</option>
							<option value="1">1</option>
							<option value="2">2</option>
							<option value="3">3</option>
							<option value="4">4</option>
							<option value="5">5</option>
						</select>
					</div>

					<div class="col-md-2 col-sm-6 customCol">
						<select name="children" class="wide" id="childrenForVehicle">
							<option data-display="children" value="false">childrens</option>
							<option value="0">0</option>
							<option value="1">1</option>
							<option value="2">2</option>
							<option value="3">3</option>
							<option value="4">4</option>
							<option value="5">5</option>
						</select>
					</div>

					<div class="col-md-1 col-sm-4">
						<a class="booking-button" href="javascript: ;" id="searchVehicles">Search!</a>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

<div class="section padding-top-bottom over-hide background-grey">
	<div class="container">
		<div class="row justify-content-center">
			<% if(vehicles.length == 0){ %>
				<h4>No Vehicles Available.</h4>
				<% } %>  

			<% vehicles.forEach( vehicle => { %> 
			<div class="col-md-6 mt-4 mb-4 mt-md-0" data-scroll-reveal="enter bottom move 50px over 0.7s after 0.2s">
				<div class="room-box background-white">
					<div class="room-name"><%= vehicle.categoryName %></div>
					<img src="/uploads/<%= vehicle.gallery[0] ? vehicle.gallery[0] : 'noImage.png' %> " alt="">
					<div class="room-box-in">
						<h5 class=""><%= vehicle.model %></h5>
						<p class="mt-3"><%= vehicle.description.substr(0, 100) + '....' %></p>
						<a class="mt-1 btn btn-primary" href="/Vehicles/vehicleBooking/<%= vehicle.id %>">book car</a>
						<!-- <a class="mt-1 btn btn-info" href="/Appartments/vehicleBooking">Full Info</a> -->
					</div>
				</div>
			</div>
			<% }) %> 			
			
		</div>
	</div>
</div>

