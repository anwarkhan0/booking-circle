<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>Users</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item active">Users List</li>
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-md-12">
            <div class="card card-success">
                <div class="card-header">
                    <h3 class="card-title">Users List</h3>
                  </div>
                  <!-- /.card-header -->
                  <div class="card-body">
                    <table id="tableData" class="table table-striped table-hover">
                      <thead>
                        <tr>
                            <th>S#</th>
                            <th>Name</th>
                            <th>Contact</th>
                            <th>CNIC</th>
                            <th>Gender</th>
                            <th>Area</th>
                            <th>Status</th>
                            <th>Action</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% for(let i= 0; i< users.length; i++) { %> 
                        <tr id="<%= users[i].id %>">
                            <td><%= i + 1 %> .</td>
                            <td><%= users[i].name %> </td>
                            <td><%= users[i].contact %> </td>
                            <td><%= users[i].CNIC %> </td>
                            <td><%= users[i].gender %> </td>
                            <td><%= users[i].location %> </td>
                            <td><span class="badge badge-secondary">Inactive</span></td>
                            <td>
                                <a href="<%= '/admin/Users/editUser/' + users[i].id %>" class="btn btn-success btn-sm">Edit &nbsp;<i class="fa fa-pencil"></i></a>
                                <button class="btn btn-danger btn-sm" onClick="<%= `deleteHandler('/admin/Users/deleteUser', ${users[i].id.toString()}` %>"><i class="fa fa-trash"></i></button>
                            </td>
                        </tr>
                        <% } %> 
                      </tbody>
                      <tfoot>
                        <tr>
                            <th>S#</th>
                            <th>Name</th>
                            <th>Contact</th>
                            <th>CNIC</th>
                            <th>Gender</th>
                            <th>Area</th>
                            <th>Status</th>
                            <th>Action</th>
                        </tr>
                      </tfoot>
                    </table>
                  </div>
            </div>
          </div>
        </div>
        <!-- /.row -->
      </div>
      <!-- /.container-fluid -->
    </section>
    <!-- /.content -->
  </div>