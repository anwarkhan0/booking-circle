      function deleteHandler(url, id) {
        if (confirm('The selected Item will be deleted Permanantly')) {
          fetch(url, {
            method: "POST",
            body: JSON.stringify({
              id: id
            }),
            headers: {
              "Content-type": "application/json; charset=UTF-8"
            }
          })
            // Checking Status
            .then(response => {
              if (response.status === 200) {
                $('#' + id).remove();
              } else {
                alert('something went wrong')
              }
            })
        }
      }

