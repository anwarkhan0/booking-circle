                alert('oop\'s something went wrong, Please Try again')
              }
            })
        }
      }

      function deleteImageHandler(url, id, image, elemId) {
        if (confirm('The selected Item will be deleted Permanantly')) {
          fetch(url, {
            method: "POST",
            body: JSON.stringify({
              id: id,
              image: image
            }),
            headers: {
              "Content-type": "application/json; charset=UTF-8"
            }
          })
            // Checking Status
            .then(response => {
              if (response.status === 200) {
                $('#' + elemId).remove();
              } else {
                alert('oop\'s something went wrong, Please Try again')
              }
            })
        }
      }

